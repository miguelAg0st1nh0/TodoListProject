@page "/"
@rendermode InteractiveServer

<PageTitle>Home</PageTitle>

<div>
    
    <h1>Welcome to the Todo List Application!</h1>
    <p>Manage your tasks efficiently and stay organized.</p>
    <p>@DefaultExamples()</p>
